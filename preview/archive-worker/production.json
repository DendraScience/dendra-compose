{
  "clients": {
    "stan": {
      "client": "STAN_CLIENT",
      "cluster": "test-cluster",
      "opts": {
        "uri": "STAN_URI"
      }
    }
  },
  "connections": {
    "archiveStore": {
      "url": "JSON_API_URL"
    }
  },
  "databases": {
    "nedb": {
      "cache": {
        "autocompactionInterval": 1020000,
        "paginate": {
          "default": 20,
          "max": 2000
        },
        "path": "/home/node/cache"
      },
      "state": {
        "autocompactionInterval": 1380000,
        "paginate": {
          "default": 20,
          "max": 2000
        },
        "path": "/data/state"
      }
    }
  },
  "port": 8080,
  "tasks": {
    "cache_grooming": null,
    "worker": {
      "agents": {
        "archive": {
          "afterSeconds": 50,
          "member": "archive",
          "module": "@dendra-science/worker-tasks-dpe",
          "options": {
            "interval": 500,
            "maxExecutions": 100
          }
        },
        "deployState": {
          "afterSeconds": 20,
          "member": "deployState",
          "module": "@dendra-science/worker-tasks-ops",
          "options": {
            "interval": 500,
            "maxExecutions": 100
          },
          "props": {
            "sources": [
              {
                "description": "Deploy worker state following a GitHub push event",
                "sub_to_subject": "github.webhook"
              }
            ],
            "specs": [
              {
                "owner": "DendraScience",
                "path": "DEPLOY_STATE_PATH",
                "ref": "refs/heads/master",
                "repo": "dendra-worker-state"
              }
            ]
          }
        }
      },
      "timerSeconds": [
        30,
        40
      ]
    }
  }
}